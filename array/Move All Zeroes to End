// Problem: Move All Zeroes to End
// Platform: GFG
// Time Complexity: O(n)
// Space Complexity: O(1)

class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        int j = 0;  // position for next non-zero element
        
        for(int i = 0; i < arr.size(); i++) {
            if(arr[i] != 0) {
                swap(arr[i], arr[j]);
                j++;
            }
        }
    }
};
